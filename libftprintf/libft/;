#include "libft.h"

void	ft_print_int(int **tab, int x, int y)
{
	int	i;
	int	j;

	i = 0;
	while (i < y)
	{
		j = 0;
		while (j < x)
		{
			ft_putnbr(tab[i][j]);
			j++;
		}
		ft_putchar("\n");
		i++;
	}
}
